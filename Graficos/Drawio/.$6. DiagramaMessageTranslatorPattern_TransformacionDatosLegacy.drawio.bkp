        <mxCell id="_qi1TiP2SetVGzzagOvf-9" value="@Component&#xa;public class EbcdicToJsonTranslator {&#xa;    &#xa;    @Autowired&#xa;    private EbcdicDecoder ebcdicDecoder;&#xa;    &#xa;    public ClienteEnergiaDTO translate(&#xa;        byte[] ebcdicData) {&#xa;        &#xa;        // Decodificar EBCDIC a ASCII&#xa;        String asciiData = ebcdicDecoder&#xa;            .decode(ebcdicData);&#xa;        &#xa;        // Parsear campos COBOL&#xa;        return ClienteEnergiaDTO.builder()&#xa;            .codigo(asciiData.substring(0,10))&#xa;            .nombre(asciiData.substring(10,60)&#xa;                .trim())&#xa;            .saldoEnergia(parseCurrency(&#xa;                asciiData.substring(60,74)))&#xa;            .estado(asciiData.charAt(74))&#xa;            .fechaUltimoPago(parseDate(&#xa;                asciiData.substring(75,83)))&#xa;            .build();&#xa;    }&#xa;}" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;align=left;fontFamily=monospace;fontSize=10;" parent="1" vertex="1">
          <mxGeometry x="988" y="313" width="220" height="360" as="geometry" />
        </mxCell>
